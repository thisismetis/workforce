<h2>Create Profile</h2>

<%= form_for(@profile) do |form| %>
  <%= form.text_field :name, placeholder: "Name" %>
  <%= form.file_field :avatar %>
  <%= form.text_area :about, placeholder: "About" %>
  Birthday: <%= form.text_field :birthday %>
  Start Date: <%= form.text_field :start_date %>
  <%= form.submit "Create Profile" %>
<% end %>


<script>
jQuery(function() {
  return $('#profile_birthday').datepicker();
});
</script>

<script>
jQuery(function() {
  return $('#profile_start_date').datepicker();
});
</script>